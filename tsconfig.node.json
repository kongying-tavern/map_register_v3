{
  "extends": "@tsconfig/node18/tsconfig.json",
  "include": [
    "vite.config.ts",
    "./plugins/**/*",
    "./.vite/**/*",
    "types/env.d.ts"
  ],
  "compilerOptions": {
    "incremental": true,
    "composite": true,
    "noEmit": true,
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "types": ["node"],
    "baseUrl": ".",
    "rootDir": ".",
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.node.tsbuildinfo"
  }
}
